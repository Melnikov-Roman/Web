<configuration>
	<system.web>
		<compilation debug="true" targetFramework="4.5" />
		<httpRuntime targetFramework="4.5" />
	</system.web>
	<system.webServer>
		<handlers>
			<add name="UrlHandler"
				preCondition="integratedMode"
				verb="*"
				path="*"
				type="ProxyServer.ProxyUrlHandler"/>
		</handlers>
	</system.webServer>

</configuration>
